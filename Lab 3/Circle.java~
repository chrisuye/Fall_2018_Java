public class Circle extends Point
{
  
  double rad;
  public Circle()
  {
    super();
    rad = 0;
  }
  public Circle(int x, int y, double r)
  {
    super(x,y);
    rad = r;
  }
  public void setRad(double r)
  {
    rad = r;
  }
  public double getrad()
  {
    return rad;
  }
  public void printRad()
  {
    System.out.println("Rad: "+rad);
  }
}